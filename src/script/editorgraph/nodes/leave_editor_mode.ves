import "blueprint.node" for Node
import "blueprint.pin" for Pin
import "blueprint.variant" for Variant
import "editorgraph.variant" for VAR_TYPE_EVENT

class LeaveEditorMode is Node
{
	init()
	{
		super.init()

		this.exports = [
			Pin(this, "event", VAR_TYPE_EVENT),
		]

		this.layout()
	}

	calc_value(idx)
	{
		import "editorgraph.event_types" for EVENT_LEAVE_EDITOR_MODE
		return Variant(VAR_TYPE_EVENT, EVENT_LEAVE_EDITOR_MODE)
	}
}