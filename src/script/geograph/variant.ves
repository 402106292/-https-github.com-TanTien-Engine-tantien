
var VAR_TYPE_GEO = "geo"
var VAR_TYPE_CONS  = "constraint"

class VarGeo
{
	init()
	{
		this.shape = nil
		this.geo_id = -1

		this.render_style = {}
	}

	clone()
	{
		var geo = VarGeo()
		
		geo.shape = this.shape.clone()

		for (var entry in this.render_style) {
			geo.render_style[entry.key] = entry.value
		}

		return geo
	}
}

class VarCons
{
	init()
	{
		this.cons = nil
		this.cons_id = -1
	}
}