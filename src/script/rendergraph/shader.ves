import "render" for Render
import "blueprint.node" for Node
import "blueprint.pin" for Pin

class Shader is Node
{
	init() 
	{
		super.init(Shader.name)

		this.exports = [
			Pin(this, "out", "shader")
		]
		this.layout()

		this.vs = ""
		this.fs = ""
	}

	execute() {
		this.prog = Render.newShader(this.vs, this.fs)
	}
}