import "render" for Render
import "blueprint.node" for Node
import "blueprint.pin" for Pin

class VertexArray is Node
{
	init() 
	{
		super.init(VertexArray.name)

		this.exports = [
			Pin(this, "out", "va")
		]

		this.data = nil
		this.attrs = nil
	}

	execute() {
		this.prog = Render.newVertexArray(this.data, this.attrs)
	}
}