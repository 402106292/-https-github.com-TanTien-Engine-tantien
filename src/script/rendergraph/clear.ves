import "render" for Render
import "blueprint.node" for Node
import "blueprint.port" for Port

class Clear is Node
{
	init() 
	{
		super.init(Clear.name)
	
		this.imports = [
			Port(this, "prev", "port")
		]
		this.exports = [
			Port(this, "next", "port")
		]

		// "color", "depth", "stencil"
		this.masks = []

		this.values = {}
	}

	execute() {
		Render.clear(this.masks, this.values)
	}
}