import "blueprint.node" for Node
import "blueprint.pin" for Pin
import "blueprint.variant" for VAR_TYPE_ANY
import "shadergraph.variant" for VAR_TYPE_FUNCTION

class Function is Node
{
	init()
	{
		super.init(Function.name)	

		this.imports = [
			Pin(this, "ret", VAR_TYPE_ANY),
		]
		this.exports = [
			Pin(this, "func", VAR_TYPE_FUNCTION),
		]

		this.layout()
	}
}