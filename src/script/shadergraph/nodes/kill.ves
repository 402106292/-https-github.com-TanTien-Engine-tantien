import "blueprint.node" for Node
import "blueprint.pin" for Pin
import "blueprint.variant" for VAR_TYPE_PORT

class Kill is Node
{
	init()
	{
		super.init(Kill.name)

		this.exports = [
			Pin(this, "next", VAR_TYPE_PORT)
		]

		this.layout()
	}

	shader_gen(node, idx)
	{
		var bb = node.find_value("bb")
		bb.kill()
	}
}