import "blueprint.node" for Node
import "blueprint.pin" for Pin
import "blueprint.variant" for VAR_TYPE_PORT

class Return is Node
{
	init()
	{
		super.init(Return.name)

		this.imports = [
			Pin(this, "prev", VAR_TYPE_PORT),
		]

		this.layout()
	}

	shader_gen(ctx) 
	{
		super.shader_gen(ctx)
			
		ctx.bb.return_null()
	}
}