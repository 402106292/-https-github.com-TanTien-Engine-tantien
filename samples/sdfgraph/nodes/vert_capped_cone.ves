var node1 = TexCoord()

_editor.add_node(node1, -128, 59)

var node2 = Parameter()

node2.var_name = "p"

_editor.add_node(node2, -283.8114452428, -58.370209108922)

var node3 = Viewport()

_editor.add_node(node3, -127, 6)

